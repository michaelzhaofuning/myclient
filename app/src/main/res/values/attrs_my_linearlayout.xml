        <attr name="titleOfMyLinearLayout" format="string"/>
        <attr name="dividerShow" format="boolean"/>
        <attr name="expandedOfMyLinearLayout" format="boolean"/>
        <attr name="contentOfMyLinearLayout" format="string"/>
        <attr name="decoratedDrawableOfMyLinearLayout" format="reference"/>

    </declare-styleable>

    <declare-styleable name="MyEditLinearLayout">
        <attr name="titleOfMyEditLinearLayout" format="string"/>
        <attr name="textColorOfTitle" format="color"/>
        <attr name="dividerShowEdit" format="boolean"/>
        <attr name="contentOfMyEditLinearLayout" format="string"/>
        <attr name="decoratedDrawable" format="reference"/>
        <attr name="formNameOfMyEditLinearLayout" format="string"/>
        <attr name="isNumOfMyEditLinearLayout" format="boolean"/>
    </declare-styleable>

</resources>